{
  "name": "mygarage-update",
  "version": "1.1.0",
  "description": "My Garage is an application to keep your car up to date with all of your maintenance",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
    "client": "cd client && yarn start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/efrencav/mygarage-update.git"
  },
  "author": "Efren Cavazos and Miguel Trujillo",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/efrencav/mygarage-update/issues"
  },
  "homepage": "https://github.com/efrencav/mygarage-update#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.7.0",
    "react-dates": "^18.4.1",
    "react-dom": "^16.7.0",
    "reactstrap": "^7.1.0",
    "validator": "^10.11.0"
  }
}
